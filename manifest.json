{
  "name": "Phone QR Finder",
  "version": "1.0",
  "description": "Appends phone QR codes to Google search result pages.",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/search*", "http://www.google.com/webhp?*"],
      "js": ["js/jquery-1.6.4.min.js", "js/jquery.qrcode.min.js", "js/qrAppend.js"]
    }
  ],
  "icons" : {
    "16": "images/icon16.png",
    "19": "images/icon19.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png",
    "130": "images/icon130.png",
    "550": "images/icon550.png"
  }
}